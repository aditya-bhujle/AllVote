{
  "expo": {
    "name": "BridgeV1",
    "slug": "BridgeV1",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "splash": {
      "image": "./assets/images/splash.png",
      "backgroundColor": "#007aff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android":{
      "permissions":["ACCESS_COARSE_LOCATION"]
    },
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "build": {
        "babel": {
          "include": [
            "name-of-my-shared-package-here"
          ]
        }
      }
    }
  }
}
